{
  "name": "flowi-ui",
  "version": "1.0.0",
  "description": "Dashboard for modular end-to-end machine learning systems",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psilva-leo/flowi-ui.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "dashboard",
    "dashboard-react",
    "argon-dashboard",
    "argon-dashboard-react"
  ],
  "author": "Leonardo Silva",
  "license": "MIT",
  "homepage": "/",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "server": "nodemon server.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "react-dev-utils": "10.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.12.1",
    "@mrblenny/react-flow-chart": "0.0.11",
    "axios": "^0.21.1",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "cors": "^2.8.5",
    "moment": "2.24.0",
    "node-sass": "^4.14.1",
    "nouislider": "14.1.1",
    "react": "16.12.0",
    "react-chartjs-2": "2.9.0",
    "react-copy-to-clipboard": "5.0.2",
    "react-datetime": "2.16.3",
    "react-dom": "16.12.0",
    "react-google-maps": "9.4.5",
    "react-redux": "^7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "^3.4.3",
    "react-select": "^3.1.0",
    "reactstrap": "8.4.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/googlemaps": "3.39.2",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.9.19",
    "concurrently": "^5.3.0",
    "eslint-plugin-flowtype": "3.13.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "typescript": "3.7.5"
  }
}
